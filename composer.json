{
    "name": "baywa-re-lusy/users-api-sdk",
    "description": "BayWa r.e. LUSY Users API SDK",
    "type": "library",
    "license": "proprietary",
    "homepage": "https://github.com/baywa-re-lusy/users-api-sdk",
    "require": {
        "php": ">= 8.1",
        "ext-curl": "*",
        "psr/cache": "^1.0",
        "psr/log": "^1.0",
        "psr/http-client": "^1.0",
        "psr/http-message": "^1.0",
        "laminas/laminas-diactoros": "^2.24",
        "baywa-re-lusy/jwt-authentication": "^2.1",
        "symfony/console": "^6.2",
        "psr/container": "^2.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^10.0",
        "guzzlehttp/guzzle": "^7.5",
        "ramsey/uuid": "^4.7"
    },
    "autoload": {
        "psr-4": {
            "BayWaReLusy\\UsersAPI\\SDK\\": "src/SDK/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BayWaReLusy\\UsersAPI\\Test\\": "tests"
        }
    }
